{
  "name": "starlight-gtm-workspace",
  "version": "0.0.1",
  "description": "Starlight Google Tag Manager plugin for Astro",
  "homepage": "https://github.com/trimble-oss/starlight-gtm",
  "repository": {
    "type": "git",
    "url": "https://github.com/trimble-oss/starlight-gtn.git"
  },
  "bugs": {
    "url": "https://github.com/trimble-oss/starlight-gtm/issues"
  },
  "scripts": {
    "dev": "concurrently \"pnpm --filter starlight-gtm-docs dev\" \"pnpm --filter starlight-gtm build --watch\"",
    "build": "pnpm --filter starlight-gtm build && pnpm --filter starlight-gtm-docs build",
    "preview": "pnpm --filter starlight-gtm-docs preview",
    "test": "pnpm -r test",
    "lint": "astro check --noSync && pnpm -r lint",
    "version": "pnpm changeset version && pnpm i --no-frozen-lockfile",
    "changeset": "changeset",
    "release": "changeset publish"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.5",
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.29.7",
    "@types/node": "^24.10.1",
    "astro": "catalog:",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1"
  },
  "keywords": [
    "astro",
    "starlight",
    "plugin"
  ],  
  "author": "jbendix",
  "license": "ISC",
  "packageManager": "pnpm@10.17.0"
}
